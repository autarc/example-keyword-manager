    "build": "yarn build:next",
    "build:next": "yarn clean:next && yarn cross-env NODE_ENV=production next build",
    "watch:test": "yarn test:js --watch",
    "dev": "yarn clean:next && yarn cross-env NODE_ENV=development node ./scripts/serve-app.js",
